module statix/test/Result

imports

  statix/API

  statix/lang/Layout
  statix/lang/Common
  statix/lang/Core
  statix/lang/Sugar
  statix/lang/Deprecated

  statix/runtime/ATerm
  statix/runtime/ScopeGraph

context-free sorts
  TestResult Solution

context-free syntax

  TestResult.EvalResult = <
    <Solution>

    errors
      <{EditorMessage "\n"}*>

    warnings
      <{EditorMessage "\n"}*>

    notes
      <{EditorMessage "\n"}*>
  >

  Solution.Solution = <
    substitution
      <{SubstEntry "\n"}*>

    <Analysis>
  >

context-free sorts
  Analysis

context-free syntax

  // special pretty print rule in statix/runtime/pp to support printing blob analyses too

  Analysis.Analysis = <
    analysis
      <Stx-ScopeGraph>
  >

context-free sorts
  SubstEntry

context-free syntax

  SubstEntry = [[Var] |-> [Stx-ATerm]]

context-free sorts
  EditorMessage MessagePos
  
context-free syntax

  EditorMessage = [[MessagePos] : [Stx-STRING]]

  MessagePos = "*"
