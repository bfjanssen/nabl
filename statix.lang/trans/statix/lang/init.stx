module statix/lang/init

signature
  sorts
    Constraint
    
  constructors
    CTrue    : Constraint
    CConj    : Constraint * Constraint -> Constraint
    
  rules
    constraintOk : Constraint
    
    constraintOk(CTrue()).
    
    constraintOk(CConj(l, r)) :-
        constraintOk(l),
        constraintOk(r).